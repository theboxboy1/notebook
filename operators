class Main {
  public static void main(String[] args) {
  
    int a,b; // declaration
    a = 7; // initializiation
    b = 3;
    int x = a/b; // modulo operator (remainder)
   double y = a/b;

  System.out.println("Integer value is " + x);
  System.out.println("Decimal value is " + y);
  System.out.println(x > y);


  
  }
}
